# Environment configuration template for M4hub Mobile

# Copy this file to .env.local and update with your actual values
# The .env.local file is not tracked by git

# Backend API Configuration
EXPO_PUBLIC_API_URL=http://localhost:8080
EXPO_PUBLIC_API_TIMEOUT=30000

# App Configuration
EXPO_PUBLIC_APP_NAME=M4hub
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_ENV=development

# Feature Flags
EXPO_PUBLIC_ENABLE_LOGGING=true
EXPO_PUBLIC_ENABLE_DEVTOOLS=true

# API Endpoints
EXPO_PUBLIC_ITEMS_ENDPOINT=/api/items
EXPO_PUBLIC_AUTH_ENDPOINT=/api/auth

# Optional: Analytics
# EXPO_PUBLIC_ANALYTICS_KEY=your_key_here

# Optional: Firebase/Push Notifications
# EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
# EXPO_PUBLIC_FIREBASE_API_KEY=your_api_key
